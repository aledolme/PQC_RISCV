#include <stdio.h>
#include <inttypes.h>
#include <string.h>

#include "keccak.h"

int main(){

	printf("Keccak Permutation begins!\n");	

	uint64_t Din[25];
	memset(Din, 0, sizeof(Din));
	
 
	Din[0] = 0xEC4AFF517369C667;
	Din[1] = 0x00000010ABBACD29;
	Din[15] = 0x8000000000000000;

		
	printf("\n Input state before Keccak-permutation is:\n");
	for (int i = 0; i<25; i+=1){
		printf("%" PRIx64 "\n", Din[i]);
	}
	printf("-----\n");

         KeccakF1600_StatePermute(Din);	
        
	printf("\n-----\nOutput state after Keccak-permutation is:\n");
	for (int i = 0; i<25; i+=1){
		printf("%" PRIx64 "\n", Din[i]);
	}
	
	return 0;

}
